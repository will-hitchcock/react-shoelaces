{
  "name": "react-shoelaces",
  "version": "0.0.3",
  "description": "Shoelaces for react",
  "main": "lib/react-shoelaces.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "webpack --env build",
    "dev": "webpack --progress --colors --watch --env dev",
    "test": "jest --coverage",
    "test:watch": "jest --watchAll"
  },
  "jest": {
    "setupFiles": [
      "./test/setup.js"
    ],
    "snapshotSerializers": [
      "<rootDir>/node_modules/enzyme-to-json/serializer"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/index.{js,jsx}"
    ]
  },
  "devDependencies": {
    "babel-cli": "6.24.1",
    "babel-core": "6.24.1",
    "babel-eslint": "7.2.3",
    "babel-loader": "7.0.0",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-plugin-transform-object-rest-spread": "~6.6.5",
    "babel-plugin-transform-react-constant-elements": "6.9.1",
    "babel-plugin-transform-react-inline-elements": "6.8.0",
    "babel-plugin-transform-react-remove-prop-types": "0.2.11",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.16.0",
    "chai": "3.5.0",
    "enzyme": "^2.9.1",
    "enzyme-to-json": "^2.0.0",
    "eslint": "3.19.0",
    "eslint-loader": "1.7.1",
    "eslint-plugin-react": "^7.3.0",
    "jest": "^21.0.2",
    "react-test-renderer": "^15.6.1",
    "webpack": "3.1.0",
    "yargs": "7.1.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/will-hitchcock/react-shoelaces.git"
  },
  "keywords": [
    "grid",
    "css-in-js",
    "styled-components",
    "react",
    "flexbox"
  ],
  "author": "Will Hitchcock",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/will-hitchcock/react-shoelaces/issues"
  },
  "homepage": "http://shoelaces-js.com",
  "dependencies": {
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "shoelaces": "0.1.0",
    "styled-components": "^2.1.2"
  }
}
